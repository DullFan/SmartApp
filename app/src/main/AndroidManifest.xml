        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Base.Theme.MyApplication"
        android:usesCleartextTraffic="true"
        tools:replace="android:icon,android:theme"
        tools:targetApi="31">
        <activity
            android:name=".ui.activity.weather.WeatherPolylineActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.robot.RobotActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.user.UserDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.user.AllUserActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.my.FunctionSwitchActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.recharge.RechargeRecordActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.my.AboutActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.usermanagement.UserManagementActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.recharge.RechargeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.my.ChangePasswordActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.my.PersonalInformationActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.login.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.welcome.WelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.MainActivity"
            android:exported="true" />
        <activity android:name=".ui.activity.newlist.WebUrlActivity" />
